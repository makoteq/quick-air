{
    "name": "quick-air",
    "version": "1.0.0",
    "description": "Check air quality near you quickly, using Airly API",
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "next dev",
        "start": "next start",
        "test": "xo && ava"
    },
    "ava": {
        "require": [
            "@babel/register",
            "./test/helpers/_setup-test-env.js",
            "./test/helpers/_ignore-utils.js"
        ]
    },
    "xo": {
        "envs": [
            "node",
            "browser"
        ],
        "extends": "xo-react",
        "nodeVersion": ">=10",
        "parser": "babel-eslint",
        "rules": {
            "import/no-unassigned-import": "off",
            "import/no-unresolved": "off",
            "react/jsx-child-element-spacing": "off",
            "no-negated-condition": "off"
        },
        "settings": {
            "react": {
                "version": "16.12"
            }
        }
    },
    "dependencies": {
        "@chakra-ui/core": "^0.5.1",
        "@emotion/core": "^10.0.22",
        "@emotion/styled": "^10.0.23",
        "airly": "^3.1.0",
        "chart.js": "^2.9.3",
        "core-js": "^3.5.0",
        "date-fns": "^2.8.1",
        "emotion-theming": "^10.0.19",
        "haversine": "^1.1.1",
        "idb-keyval": "^3.2.0",
        "leaflet": "^1.6.0",
        "next": "^9.1.6",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-chartjs-2": "^2.8.0",
        "react-dom": "^16.12.0",
        "react-leaflet": "^2.6.1",
        "react-use-form-state": "^0.12.0"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/register": "^7.7.4",
        "ava": "^2.4.0",
        "babel-eslint": "^10.0.3",
        "babel-plugin-emotion": "^10.0.23",
        "browser-env": "^3.3.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint-config-xo-react": "^0.20.0",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "imagemin-svgo": "^7.0.0",
        "next-fonts": "^0.19.0",
        "next-offline": "^4.0.6",
        "next-optimized-images": "^2.5.4",
        "require-hacker": "^3.0.1",
        "xo": "^0.25.3"
    }
}
